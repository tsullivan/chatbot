{
  "compilerOptions": {
    "lib": [
      "es6"
    ],
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es6",
    "plugins": [
      { "name": "typescript-tslint-plugin" }
    ],
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "pretty": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "alwaysStrict": true
  }
}
