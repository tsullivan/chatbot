{
  "name": "chatbot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "yarn run lint && yarn run jest",
    "dev": "DEBUG='chatbot:*' NODE_ENV=development nodemon -e js,ts,json index.js --web",
    "dev-slack": "DEBUG='chatbot:*' NODE_ENV=development nodemon -e js,ts,json index.js --slack --web=false",
    "jest": "NODE_ENV=test SESSION_SECRET=testsess APM_TOKEN=testtoke  SLACK_BOT_TOKEN=testtoke jest --ci --verbose '\\.test\\.[jt]s'",
    "lint": "eslint --ignore-pattern='*json' 'public/js/**' 'src/**' 'test/**' 'config/**'"
  },
  "husky": {
    "hooks": {
      "precommit": "yarn run lint",
      "prepush": "yarn run test"
    }
  },
  "author": "Tim Sullivan",
  "license": "ISC",
  "engines": {
    "node": ">=10.14 <12"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "prettier": {
    "printWidth": 88,
    "singleQuote": true,
    "trailingComma": "es5",
    "parser": "typescript"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "eslint": "~5.10",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-node": "~8.0",
    "eslint-plugin-prettier": "~3.0",
    "husky": "^2.2.0",
    "jest": "~23.6",
    "nodemon": "~1.18",
    "prettier": "~1.15",
    "supertest": "~3.3",
    "ts-jest": "~23.10"
  },
  "dependencies": {
    "@slack/client": "~4.8",
    "@types/cookie-parser": "~1.4",
    "@types/debug": "^4.1.4",
    "@types/express": "~4.16",
    "@types/express-session": "~1.15",
    "@types/jest": "~23.3",
    "@types/lodash": "~4.14",
    "@types/node": "~10.12",
    "@types/supertest": "~2.0",
    "@types/yargs": "^13.0.0",
    "body-parser": "~1.18",
    "cookie-parser": "~1.4",
    "debug": "~4.1",
    "elastic-apm-node": "~2.10",
    "express": "~4.16",
    "express-session": "~1.15",
    "lodash": "~4.17",
    "moment": "~2.23",
    "strip-newlines": "~1.0",
    "ts-node": "~7.0",
    "typescript": "~3.2",
    "yargs": "~12.0"
  }
}
