{
  "name": "chatbot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "yarn run lint && yarn run jest",
    "dev": "DEBUG='chatbot:*' NODE_ENV=development nodemon -e js,ts,json index.js --web",
    "dev-slack": "DEBUG='chatbot:*' NODE_ENV=development nodemon -e js,ts,json index.js --slack --web=false",
    "jest": "NODE_ENV=test jest --ci --verbose '\\.test\\.[jt]s'",
    "lint": "eslint . && tslint -p tsconfig.json"
  },
  "husky": {
    "hooks": {
      "precommit": "yarn run lint",
      "prepush": "yarn run test"
    }
  },
  "author": "Tim Sullivan",
  "license": "ISC",
  "engines": {
    "node": ">=8.11 <9"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "prettier": {
    "printWidth": 88,
    "singleQuote": true,
    "trailingComma": "es5",
    "parser": "typescript"
  },
  "devDependencies": {
    "eslint": "5.10.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-prettier": "3.0.0",
    "husky": "1.3.0",
    "jest": "23.6.0",
    "nodemon": "1.18.9",
    "prettier": "1.15.3",
    "prettier-tslint": "0.4.1",
    "supertest": "3.3.0",
    "ts-jest": "23.10.5",
    "tslint": "5.12.0",
    "tslint-config-prettier": "1.17.0"
  },
  "dependencies": {
    "@slack/client": "4.8.0",
    "@types/cookie-parser": "1.4.1",
    "@types/express": "4.16.0",
    "@types/jest": "23.3.10",
    "@types/node": "10.12.18",
    "body-parser": "1.18.3",
    "cookie-parser": "1.4.3",
    "debug": "4.1.0",
    "elastic-apm-node": "2.0.6",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "lodash": "4.17.11",
    "moment": "2.23.0",
    "strip-newlines": "1.0.1",
    "ts-node": "7.0.1",
    "typescript": "3.2.2",
    "yargs": "12.0.5"
  }
}
